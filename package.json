{
  "name": "dicty-components-login",
  "version": "0.0.0-semantically-released",
  "description": "React library that displays login buttons for dictyBase web applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "private": true,
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "flow": true,
          "typescript": false
        }
      ],
      [
        "@babel/env"
      ],
      "@babel/react"
    ]
  },
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest src",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release",
    "flow": "flow",
    "lint": "eslint ./src --ext .js,.jsx",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@material-ui/core": "^4.11.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "flow-bin": "^0.152.0",
    "husky": "6",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "nwb": "^0.23.0",
    "prettier": "^2.3.1",
    "react": "^16.14.0",
    "react-dev-utils": "^11.0.4",
    "react-dom": "^16.14.0",
    "react-styleguidist": "^11.1.6",
    "react-test-renderer": "^16.14.0",
    "semantic-release": "^17.4.3",
    "sinon": "^11.1.1",
    "typeface-roboto": "1.1.13"
  },
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/dicty-components-login.git"
  },
  "keywords": [
    "react-component"
  ]
}
